{
  "compilerOptions": {
    "module": "es2015",
    "target": "es5",
    "outDir": "es2015",
    "lib": ["dom", "es5"],
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "declarationDir": "typings",
    "newLine": "LF",
    "strictNullChecks": true
  },
  "include": [
    "src/**/*"
  ]
}
